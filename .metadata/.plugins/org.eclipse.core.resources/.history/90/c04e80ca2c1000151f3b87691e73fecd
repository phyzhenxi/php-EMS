<?php
header("Content-type:text/html;charset=utf-8");
//永久转换变量
$num1=1.1;
$s=settype($num1, 'boolean');

var_dump($num1);
var_dump($s);












var_dump($num1);
var_dump($s);

//临时转换变量
$num2=9.9;
$str='1';
$result=(int)$str+(int)$num2;
var_dump($result);
echo '<br/>';
var_dump($num2);
echo '<br/>';
var_dump($str);
echo '<br/>';